+            int i = 0;
+            while (enumerator1.MoveNext())
+            {
+                i++;
+                enumerator1.Current.Key.ShouldBe("Key1");
+                enumerator1.Current.Value.ShouldBe("Value1");
+            }
+            i.ShouldBe(dict.Count);
+
+            IDictionaryEnumerator enumerator2 = ((IDictionary)dict).GetEnumerator();
