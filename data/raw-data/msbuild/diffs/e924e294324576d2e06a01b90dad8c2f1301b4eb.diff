+            dict.Count.ShouldBe(2);
+            dict = dict.Clear();
+            dict.Count.ShouldBe(0);
+        }
+
+        [Fact]
+        public void IndexerReturnsPreviouslySetItem()
+        {
+            ImmutableDictionary dict = _emptyDict.SetItem("Key1", "Value1");
