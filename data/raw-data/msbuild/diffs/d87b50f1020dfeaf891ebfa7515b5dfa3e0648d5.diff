-                Message = Message.Substring(0, MAX_LENGTH - 1) + "…";
-            }
-            // Get type
-            switch (args)
-            {
-                case BuildMessageEventArgs:
-                    Type = MessageType.HighPriorityMessage;
-                    break;
-                case BuildWarningEventArgs warning:
-                    Type = MessageType.Warning;
